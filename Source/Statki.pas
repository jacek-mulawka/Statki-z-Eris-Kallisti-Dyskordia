        zt_statek_l := statek_f;
        zasiêg__broñ_l := 0;

        for i_l := 0 to Length( zt_statek_l.artyleria_t ) - 1 do
          if zasiêg__broñ_l < zt_statek_l.artyleria_t[ i_l ].zasiêg__broñ then
