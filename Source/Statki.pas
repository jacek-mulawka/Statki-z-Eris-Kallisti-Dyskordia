      pokój_rozmów_r_l.id_odbiorca := statek__strzelaj¹cy_f.id_gracz;


      if   ( czy_gra_lokalna_g )
        or (
                 ( czy_serwer_g )
             and ( pokój_rozmów_r_l.id_odbiorca = serwer_peer_port_c )
