                   ( not torpedy_wyrzutnia_f.si__cel__wyznaczony )
               and (  Czas_Miêdzy_W_Sekundach( torpedy_wyrzutnia_f.si__cel__wyszukanie_ostatnie_sekundy_i ) < si__cel__wyszukanie_sekundy_c * 0.1  )
