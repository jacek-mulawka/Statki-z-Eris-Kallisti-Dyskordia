    milisekund_prze³adowania_l := Czas_Miêdzy_W_Milisekundach( Self.strza³_czas__lufa_sekundy_i ) + Self.milisekund_prze³adowania_do_uszkodzenia;


  if Self.amunicja_pobrana then
