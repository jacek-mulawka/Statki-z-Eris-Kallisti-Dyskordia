        //    ) // Prêdkoœæ jest pomniejszana o tyle procent.
        //  * 0.85; // Modyfikuje si³ê wp³ywu kolizji na prêdkoœæ.


        if Self.prêdkoœæ__kolizja_zwalnianie >= 100 then
