                  statek_f_f.si__torpedy_alarm__odleg³oœæ := odleg³oœæ_l;
                  statek_f_f.si__torpedy_alarm__id_amunicja__unikana := id_amunicja_najbli¿sza_l;

                  if statek_f_f.si_aktywnoœæ <> sia_Walka__Torpedy_Ucieczka then
