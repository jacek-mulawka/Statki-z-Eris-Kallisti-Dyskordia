      Self.prze³adowanie_procent := 0;


  if Self.prze³adowanie_procent > 100 then
    Self.prze³adowanie_procent := 100
  else//if Self.prze³adowanie_procent > 100 then
  if Self.prze³adowanie_procent < 0 then
