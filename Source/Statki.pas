                    GLVectorGeometry.VectorMake( Self.AbsoluteDirection.X, 0, Self.AbsoluteDirection.Z ),
                    GLVectorGeometry.VectorMake( 0, 0, 0 ) // Najlepiej sprawdzaæ k¹t w punkcie zero.
                  )
