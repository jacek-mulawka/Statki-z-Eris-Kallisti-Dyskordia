  if @Self.amunicja_wystrzelona_utwórz_jeden <> nil then
    Self.amunicja_wystrzelona_utwórz_jeden( rodzic_l, Self, obracaj_dzia³a_f, cel_wspó³rzêdne_f );


  if    ( Self.dzia³o <> nil )
