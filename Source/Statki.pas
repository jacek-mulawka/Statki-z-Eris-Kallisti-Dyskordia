    //---//if Czas_Miêdzy_W_Milisekundach( Self.strza³_czas__broñ_sekundy_i ) <= Self.strza³_od_blokada_milisekundy then



  //if Self.celownik_linia_bez_falowania.NodesAspect <> Self.celownik_linia.NodesAspect then
