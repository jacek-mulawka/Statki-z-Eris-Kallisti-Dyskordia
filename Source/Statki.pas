    ztsi_2
      : single;
    zt_affine_vektor : TAffineVector;
  begin

    if   ( not SI__Strzela_CheckBox.Checked )
      or ( torpedy_wyrzutnia_f = nil )
      or (  not Assigned( torpedy_wyrzutnia_f )  )
      or ( torpedy_wyrzutnia_f.statek__tw = nil )
