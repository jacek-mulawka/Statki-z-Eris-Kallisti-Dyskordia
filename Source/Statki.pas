              // Zasiêg wykrywania torpedy.
              ztsi_2 := amunicja_prêdkoœæ_zakresy_r_g.prêdkoœæ_torpeda * 10;

              if statek_f_f.y_prymityw_najwiêksze > 1 then
                ztsi_2 := ztsi_2 + statek_f_f.y_prymityw_najwiêksze * 20;

              ztsi_2 := ztsi_2 * dzieñ_jasnoœæ_g; // Noc ogranicza widocznoœæ.

              if mg³a_intensywnoœæ_g > 0 then
                ztsi_2 := ztsi_2 * mg³a_intensywnoœæ_g * 0.01; // Mg³a ogranicza widocznoœæ.

              if ztsi_2 < amunicja_prêdkoœæ_zakresy_r_g.prêdkoœæ_torpeda then
