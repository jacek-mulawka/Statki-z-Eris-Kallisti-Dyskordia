      begin

        przynajmniej_jedna_lufa_pobra³a_amunicjê := false;
        prze³adowanie_procent_najwiêksze := 0;

        for i := 0 to Length( Self.lufy_t ) - 1 do
          if Self.lufy_t[ i ].amunicja_pobrana then
