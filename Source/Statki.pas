    else//if milisekund_prze³adowania_l < Self.prze³adowanie_czas_milisekundy__lufa * 0.25 then
      begin

        Self.korpus__lufa.Position.Z := TDzia³o(Self.dzia³o).cofanie_wystrza³_pozycja_pocz¹tkowa;

